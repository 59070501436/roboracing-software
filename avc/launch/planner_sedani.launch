<launch>
    <node name="planner_avc" pkg="rr_common" type="planner" output="screen" >
        <param name="N_PATH_SEGMENTS" value="2"/>
        <param name="N_CONTROL_SAMPLES" value="250"/>
        <param name="SMOOTHING_ARRAY_SIZE" value="1"/>
        <param name="SEGMENT_DISTANCES" value="3 4"/>
        <param name="STEER_LIMITS" value="0.42 0.42"/>
        <param name="STEER_STDDEVS" value="0.3 0.2"/>
        <param name="INPUT_CLOUD_TOPIC" value="/current_obstacles"/>
        <param name="MAX_SPEED" value="0.4"/>
        <param name="PATH_SIMILARITY_CUTOFF" value="0.15"/>
        <param name="MAX_RELATIVE_COST" value="10.0"/>

        <param name="WHEEL_BASE" value="0.482"/>
        <param name="COLLISION_RADIUS" value="0.5"/>
    </node>
</launch>
