<launch>
    <node name="planner_avc" pkg="rr_common" type="planner" output="screen" >
        <param name="N_PATH_SEGMENTS" value="2"/>
        <param name="N_CONTROL_SAMPLES" value="250"/>
        <param name="SMOOTHING_ARRAY_SIZE" value="3"/>
        <param name="SEGMENT_DISTANCES" value="3 4"/>
        <param name="DISTANCE_INCREMENT" value="0.2"/>
        <param name="STEER_LIMITS" value="0.3 0.3"/>
        <param name="STEER_STDDEVS" value="0.2 0.15"/>
        <param name="INPUT_CLOUD_TOPIC" value="/current_obstacles"/>
        <param name="MAX_SPEED" value="2.0"/>
        <param name="PATH_SIMILARITY_CUTOFF" value="0.2"/>
        <param name="MAX_RELATIVE_COST" value="10.0"/>

        <param name="WHEEL_BASE" value="0.37"/>
        <param name="OBSTACLE_SEARCH_RADIUS" value="3.0"/>
        <param name="COLLISION_DIST_FRONT" value="0.3"/>
        <param name="COLLISION_DIST_BACK" value="0.1"/>
        <param name="COLLISION_DIST_SIDE" value="0.15"/>
    </node>
</launch>
