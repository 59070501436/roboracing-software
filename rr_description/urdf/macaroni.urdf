<robot name="macaroni">

    <!--Car Links-->

    <link name="chassis">
        <inertial>
            <origin xyz="0 0 0"/>
            <mass value="5.0"/>
            <inertia ixx="0.10595165" ixy="-0.002487144" ixz="0.011981253" iyy="0.126271962" iyz="0.011832006" izz="0.033185337"/>
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://rr_description/meshes/car.dae"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://rr_description/meshes/car.dae"/>
            </geometry>
        </collision>
    </link>
    <gazebo reference="chassis">
        <material>Gazebo/Grey</material>
    </gazebo>


    <link name="back_axle">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>

    <link name="wheel_back_left">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.1" />
            <inertia  ixx="0.000405599" ixy="0.0"  ixz="0.0"  iyy="0.000405599"  iyz="0.0"  izz="0.000518265" />
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://rr_description/meshes/wheel.dae"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://rr_description/meshes/wheel.dae"/>
            </geometry>
        </collision>
    </link>
    <gazebo reference="wheel_back_left">
        <mu1>0.7</mu1>
        <mu2>0.75</mu2>
        <material>Gazebo/Grey</material>
    </gazebo>


    <link name="wheel_back_right">
        <inertial>
            <origin rpy="0 0 0" xyz="0 0 0" />
            <mass value="0.1" />
            <inertia  ixx="0.000405599" ixy="0.0"  ixz="0.0"  iyy="0.000405599"  iyz="0.0"  izz="0.000518265" />
        </inertial>
        <visual>
            <origin rpy="0 0 3.14" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://rr_description/meshes/wheel.dae"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://rr_description/meshes/wheel.dae"/>
            </geometry>
        </collision>
    </link>
    <gazebo reference="wheel_back_right">
        <mu1>0.7</mu1>
        <mu2>0.75</mu2>
        <material>Gazebo/Grey</material>
    </gazebo>


    <link name="steering_front_left">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>

    <link name="wheel_front_left">
        <inertial>
            <origin xyz="0 0 0" />
            <mass value="0.1" />
            <inertia  ixx="0.000405599" ixy="0.0"  ixz="0.0"  iyy="0.000405599"  iyz="0.0"  izz="0.000518265" />
        </inertial>
        <visual>
            <origin rpy="0 0 0" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://rr_description/meshes/wheel.dae"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://rr_description/meshes/wheel.dae"/>
            </geometry>
        </collision>
    </link>
    <gazebo reference="wheel_front_left">
        <mu1>0.7</mu1>
        <mu2>0.75</mu2>
        <material>Gazebo/Grey</material>
    </gazebo>

    <link name="steering_front_right">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>

    <link name="wheel_front_right">
        <inertial>
            <origin xyz="0 0 0" />
            <mass value="0.1" />
            <inertia  ixx="0.000405599" ixy="0.0"  ixz="0.0"  iyy="0.000405599"  iyz="0.0"  izz="0.000518265" />
        </inertial>
        <visual>
            <origin rpy="0 0 3.14" xyz="0 0 0"/>
            <geometry>
                <mesh filename="package://rr_description/meshes/wheel.dae"/>
            </geometry>
        </visual>
        <collision>
            <geometry>
                <mesh filename="package://rr_description/meshes/wheel.dae"/>
            </geometry>
        </collision>
    </link>
    <gazebo reference="wheel_front_right">
        <mu1>0.7</mu1>
        <mu2>0.75</mu2>
        <material>Gazebo/Grey</material>
    </gazebo>


    <!--Sensor Links-->

    <link name="laser"/>
    <gazebo reference="laser">
        <sensor type="ray" name="lidar">
            <visualize>false</visualize>
            <update_rate>15</update_rate>
            <ray>
                <scan>
                    <horizontal>
                        <samples>270</samples>
                        <resolution>1</resolution>
                        <min_angle>-2.35619</min_angle>
                        <max_angle>2.35619</max_angle>
                    </horizontal>
                </scan>
                <range>
                    <min>0.05</min>
                    <max>8.00</max>
                    <resolution>0.02</resolution>
                </range>
            </ray>
            <plugin name="lidar_plugin" filename="libgazebo_ros_laser.so">
                <robotNamespace>/</robotNamespace>
                <topicName>/scan</topicName>
                <frameName>/laser</frameName>
            </plugin>
        </sensor>
    </gazebo>


    <link name="imu"/>
    <gazebo reference="imu">
        <gravity>true</gravity>
        <sensor name="imu_sensor" type="imu">
            <always_on>true</always_on>
            <update_rate>30</update_rate>
            <visualize>true</visualize>
            <topic>imu</topic>
            <plugin filename="libgazebo_ros_imu_sensor.so" name="imu_plugin">
                <topicName>imu</topicName>
                <bodyName>imu_link</bodyName>
                <updateRateHZ>30.0</updateRateHZ>
                <gaussianNoise>0.005</gaussianNoise>
                <xyzOffset>0 0 0</xyzOffset>
                <rpyOffset>0 0 0</rpyOffset>
                <frameName>imu_link</frameName>
            </plugin>
            <pose>0 0 0 0 0 0</pose>
        </sensor>
    </gazebo>

    <link name="camera_center">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    <gazebo reference="camera_center">
        <sensor name="camera_sensor" type="camera">
            <camera>
                <horizontal_fov>1.02974<!-- 59 deg in rad --></horizontal_fov>
                <image>
                    <width>1280</width>
                    <height>964</height>
                    <format>R8G8B8</format>
                </image>
                <clip>
                    <near>0.05</near>
                    <far>100</far>
                </clip>
                <noise>
                  <type>gaussian</type>
                  <!-- Noise is sampled independently per pixel on each frame.
                       That pixel's noise value is added to each of its color
                       channels, which at that point lie in the range [0,1]. -->
                  <mean>0.0</mean>
                  <stddev>0.007</stddev>
                </noise>
            </camera>
            <always_on>true</always_on>
            <update_rate>30</update_rate>
            <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
                <robotNamespace>/</robotNamespace>
                <baseline>0.1</baseline>
                <alwaysOn>true</alwaysOn>
                <updateRate>30.0</updateRate>
                <cameraName>camera_center</cameraName>
                <imageTopicName>image_raw</imageTopicName>
                <cameraInfoTopicName>camera_info</cameraInfoTopicName>
                <frameName>camera_optical_center</frameName>
                <distortionK1>0.00000001</distortionK1>
                <distortionK2>0.00000001</distortionK2>
                <distortionK3>0.00000001</distortionK3>
                <distortionT1>0.00000001</distortionT1>
                <distortionT2>0.00000001</distortionT2>
            </plugin>
        </sensor>
    </gazebo>

    <link name="camera_optical_center"/>

    <link name="camera_left">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    <gazebo reference="camera_left">
        <sensor name="camera_sensor" type="camera">
            <camera>
                <horizontal_fov>1.02974<!-- 59 deg in rad --></horizontal_fov>
                <image>
                    <width>640</width>
                    <height>480</height>
                    <format>R8G8B8</format>
                </image>
                <clip>
                    <near>0.05</near>
                    <far>100</far>
                </clip>
                <noise>
                  <type>gaussian</type>
                  <!-- Noise is sampled independently per pixel on each frame.
                       That pixel's noise value is added to each of its color
                       channels, which at that point lie in the range [0,1]. -->
                  <mean>0.0</mean>
                  <stddev>0.007</stddev>
                </noise>
            </camera>
            <always_on>true</always_on>
            <update_rate>30</update_rate>
            <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
                <robotNamespace>/</robotNamespace>
                <baseline>0.1</baseline>
                <alwaysOn>true</alwaysOn>
                <updateRate>30.0</updateRate>
                <cameraName>camera_left</cameraName>
                <imageTopicName>image_raw</imageTopicName>
                <cameraInfoTopicName>camera_info</cameraInfoTopicName>
                <frameName>camera_optical_left</frameName>
                <distortionK1>0.00000001</distortionK1>
                <distortionK2>0.00000001</distortionK2>
                <distortionK3>0.00000001</distortionK3>
                <distortionT1>0.00000001</distortionT1>
                <distortionT2>0.00000001</distortionT2>
            </plugin>
        </sensor>
    </gazebo>

    <link name="camera_optical_left"/>

    <link name="camera_right">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    <gazebo reference="camera_left">
        <sensor name="camera_sensor" type="camera">
            <camera>
                <horizontal_fov>1.02974<!-- 59 deg in rad --></horizontal_fov>
                <image>
                    <width>640</width>
                    <height>480</height>
                    <format>R8G8B8</format>
                </image>
                <clip>
                    <near>0.05</near>
                    <far>100</far>
                </clip>
                <noise>
                  <type>gaussian</type>
                  <!-- Noise is sampled independently per pixel on each frame.
                       That pixel's noise value is added to each of its color
                       channels, which at that point lie in the range [0,1]. -->
                  <mean>0.0</mean>
                  <stddev>0.007</stddev>
                </noise>
            </camera>
            <always_on>true</always_on>
            <update_rate>30</update_rate>
            <plugin name="camera_controller" filename="libgazebo_ros_camera.so">
                <robotNamespace>/</robotNamespace>
                <baseline>0.1</baseline>
                <alwaysOn>true</alwaysOn>
                <updateRate>30.0</updateRate>
                <cameraName>camera_right</cameraName>
                <imageTopicName>image_raw</imageTopicName>
                <cameraInfoTopicName>camera_info</cameraInfoTopicName>
                <frameName>camera_optical_right</frameName>
                <distortionK1>0.00000001</distortionK1>
                <distortionK2>0.00000001</distortionK2>
                <distortionK3>0.00000001</distortionK3>
                <distortionT1>0.00000001</distortionT1>
                <distortionT2>0.00000001</distortionT2>
            </plugin>
        </sensor>
    </gazebo>

    <link name="camera_optical_right"/>


    <link name="ultrasonic_array_base">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    <link name="ultrasonic_0">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    <link name="ultrasonic_1">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    <link name="ultrasonic_2">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    <link name="ultrasonic_3">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
    </link>
    <link name="ultrasonic_4">
        <inertial> <!--Null inertia-->
            <mass value="0.001"/>
            <inertia ixx="0.001" ixy="0" ixz="0" iyy="0.001" iyz="0" izz="0.001"/>
        </inertial>
