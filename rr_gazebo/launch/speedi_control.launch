<launch>

    <!-- Load joint controller configurations from YAML file to parameter server -->
    <rosparam file="$(find rr_gazebo)/config/speedi_control.yaml" command="load"/>

    <!-- load the controllers -->
    <node name="controller_spawner" pkg="controller_manager" type="spawner" respawn="false"
          output="screen" ns="/roboracing" args="joint_state_controller axle_effort_controller left_steer_position_controller right_steer_position_controller"/>

</launch>